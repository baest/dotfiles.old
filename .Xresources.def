!Do not edit .Xresources directly, but edit .Xresources.def and run cpp -P .Xresources.def .Xresources && xrdb ~/.Xresources
URxvt.perl-lib: /home/mfk/ressources/urxvt

!urxvt.perl-ext-common:default,matcher,searchable-scrollback,tabbedex
urxvt.perl-ext-common:default,matcher
!,overlay-osc
!urxvt.perl-ext-common:default,matcher,searchable-scrollback,tabbedex,urllauncher
urxvt.tabbed.new-button: false
urxvt.tabbed.autohide: false
urxvt.url-launcher: /usr/bin/google-chrome
urxvt.matcher.button: 1

urxvt.keysym.Control-t: perl:tabbedex:new_tab
urxvt.keysym.Control-Tab: perl:tabbedex:next_tab
urxvt.keysym.Control-Shift-Tab: perl:tabbedex:prev_tab
urxvt.keysym.Control-Left: perl:tabbedex:prev_tab
urxvt.keysym.Control-Right: perl:tabbedex:next_tab

!URxvt.font: xft:ubuntu:size=10 
!urxvt.font: -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-2
!urxvt.letterSpace: -1
!urxvt*font: xft:Monospace:pixelsize=14

!urxvt*font: -*-terminus-*-*-*-*-*-120-*-*-*-*-*-*
!Xft.dpi: 96
!Xft.antialias: true
!Xft.hinting: full
!Xft.hintstyle: hintslight
!Xft.rgba: rgb

urxvt.font: xft:terminus:pixelsize=10
urxvt.boldFont: xft:terminus:pixelsize=10:bold
!urxvt*font: xft:Terminess Powerline:size=8:hinting=True;antialias:True
!urxvt*boldFont: xft:Terminess Powerline:size=8:bold;hinting=True;antialias:True

urxvt.scrollstyle: plain
urxvt.secondaryScroll: true
urxvt.scrollbar: false
!urxvt.saveLines: 65535
urxvt.saveLines: 0
!urxvt.scrollBar_right: true

urxvt.cutchars: `""''()*,;:<>[]{|}â”‚
urxvt.metaSendsEscape: true

urxvt*termName: rxvt
!urxvt.keysym.C-k:       echo -ne '\\033c'\n

! Solarized color scheme for the X Window System
!
! http://ethanschoonover.com/solarized

#define S_base03        #002b36
#define S_base02        #073642
#define S_base01        #586e75
#define S_base00        #657b83
#define S_base0         #839496
#define S_base1         #93a1a1
#define S_base2         #eee8d5
#define S_base3         #fdf6e3
#define S_yellow        #b58900
#define S_orange        #cb4b16
#define S_red           #dc322f
#define S_magenta       #d33682
#define S_violet        #6c71c4
#define S_blue          #268bd2
#define S_cyan          #2aa198
#define S_green         #859900

*background:            S_base3
*foreground:            S_base0
*fading:                40
*fadeColor:             S_base3
*cursorColor:           S_base01
*pointerColorBackground:S_base1
*pointerColorForeground:S_base01

!! black dark/light
*color0:                S_base2
*color8:                S_base3

!! red dark/light
*color1:                S_red
*color9:                S_orange

!! green dark/light
*color2:                S_green
*color10:               S_base1

!! yellow dark/light
*color3:                S_yellow
*color11:               S_base0

!! blue dark/light
*color4:                S_blue
*color12:               S_base00

!! magenta dark/light
*color5:                S_magenta
*color13:               S_violet

!! cyan dark/light
*color6:                S_cyan
*color14:               S_base01

!! white dark/light
*color7:                S_base02
*color15:               S_base03


!URxvt.intensityStyles: false
!!! Solarized ends

urxvt*geometry: 309x98+65-0

